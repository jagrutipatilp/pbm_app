    <fragment
        android:id="@+id/navigation_classes"
        android:name="com.jags.pbm.ui.classes.ClassesFragment"
        android:label="@string/title_Classes"
        tools:layout="@layout/fragment_classes" />

    <fragment
        android:id="@+id/navigation_notification"
        android:name="com.jags.pbm.ui.notification.NotificationFragment"
