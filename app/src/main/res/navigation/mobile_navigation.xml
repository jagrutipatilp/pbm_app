<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">



    <fragment
        android:id="@+id/navigation_home"
        android:name="com.jags.pbm.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />


    <fragment
        android:id="@+id/navigation_homework"
        android:name="com.jags.pbm.ui.homework.HomeworkFragment"
        android:label="@string/title_homework"
        tools:layout="@layout/fragment_homework" />

    <fragment
        android:id="@+id/navigation_attendance"
        android:name="com.jags.pbm.ui.attendance.AttendanceFragment"
        android:label="@string/title_attendance"
        tools:layout="@layout/fragment_attendance" />

    <fragment
        android:id="@+id/navigation_schedule"
        android:name="com.jags.pbm.ui.schedule.ScheduleFragment"
        android:label="@string/title_Schedule"
